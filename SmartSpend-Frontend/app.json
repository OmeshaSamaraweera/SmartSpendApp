{
  "expo": {
    "name": "SmartSpendApp",
    "slug": "SmartSpendApp",
    "icon": "./screens/images/App_Logo_imresizer.png",
    "version": "1.0.0",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "package": "com.amarbazlin.smartspend"
          },
          "ios": {
            "deploymentTarget": "13.4",
            "package": "com.amarbazlin.smartspend"
          }
        }
      ]
    ],
    "owner": "bazlinamar",
    "ios": {
      "bundleIdentifier": "y",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.bazlinamar.SmartSpend"
    },
    "extra": {
      "eas": {
        "projectId": "40310d29-8912-447a-af70-10603a438da7"
      }
    }
  }
}
