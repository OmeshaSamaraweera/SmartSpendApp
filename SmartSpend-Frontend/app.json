{
  "expo": {
    "name": "SmartSpend",
    "slug": "smartspend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./screens/images/App_Logo_imresizer.png",
    "userInterfaceStyle": "light",
    "platforms": [
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "android": {
      "package": "com.bazlinamar.smartspend",
      "permissions": [
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "extra": {
      "apiUrl": "http://192.168.75.202:5050",
      "eas": {
        "projectId": "45cbf1a9-fad6-4779-963b-1592e4a1e630"
      }
    },
    "owner": "bazlinamar"
  }
}